{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "app.py": {
      "maxDuration": 10,
      "memory": 128
    }
  }
}
